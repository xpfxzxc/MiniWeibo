{
  "name": "mini-weibo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -d src/migrations/ -n ",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -d src/migrations/ -f src/config/database.ts -n ",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run -f src/config/database.ts",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f src/config/database.ts",
    "seed:create": "ts-node node_modules/typeorm/cli.js migration:create -c seed -d src/seeds/ -n ",
    "seed:run": "ts-node node_modules/typeorm/cli.js migration:run -c seed -f src/config/database.ts",
    "seed:revert": "ts-node node_modules/typeorm/cli.js migration:revert -c seed -f src/config/database.ts",
    "seed:reseed": "npm run seed:revert && npm run seed:run",
    "migration:clear": "npm run seed:revert && npm run migration:revert",
    "migration:refresh": "npm run migration:clear && npm run migration:run",
    "migration:refreshAndSeed": "npm run migration:refresh && npm run seed:run"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.3",
    "bcrypt": "^5.0.0",
    "blueimp-md5": "^2.12.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "connect-flash": "^0.1.1",
    "connect-typeorm": "^1.1.1",
    "csurf": "^1.10.0",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.16.2",
    "helmet": "^3.21.1",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "nestjs-config": "^1.4.4",
    "nestjs-typeorm-paginate": "^1.0.2",
    "nunjucks": "^3.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "svg-captcha": "^1.4.0",
    "typeorm": "^0.2.18",
    "typeorm-static": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "4.16.1",
    "@types/faker": "^4.1.5",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "2.0.7",
    "faker": "^4.1.0",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
